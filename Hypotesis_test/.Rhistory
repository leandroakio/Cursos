cria_plot <- function(data, mapping, method = "loess", ...){
p <- ggplot(data = data, mapping = mapping) +
geom_point() +
geom_smooth(method = method, ...)
p
}
ggpairs(mtcars, lower = list(continuous = cria_plot))
